{
    "title": "yu's Complex Modifications",
    "rules": [
        {
            "description": "switch input sources",
            "manipulators": [
                {
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": [
                                "fn",
                                "left_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "japanese_kana"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "left_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "japanese_kana"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "mandatory": [
                                "fn",
                                "left_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "japanese_eisuu"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "mandatory": [
                                "left_control",
                                "left_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "japanese_eisuu"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Emacs-style key binding using the fn key",
            "manipulators": [
                {
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow",
                            "modifiers": []
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
